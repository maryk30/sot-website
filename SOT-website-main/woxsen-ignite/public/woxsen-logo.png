<script>
$(document).ready(function () {

    if (window.location.hash) {

        // Get hash WITHOUT #
        var rawHash = window.location.hash.substring(1);

        // Remove trailing slash (VERY IMPORTANT)
        rawHash = rawHash.replace(/\/+$/, '');

        // Now it's a valid ID like "coe-2816"
        var $target = document.getElementById(rawHash);

        if ($target) {

            // Close all accordions
            document.querySelectorAll('.faqanswer').forEach(el => el.style.display = 'none');
            document.querySelectorAll('.plusminus').forEach(el => el.textContent = '+');

            // Open matched accordion
            $target.querySelector('.faqanswer').style.display = 'block';
            $target.querySelector('.plusminus').textContent = '-';

        }
    }

});
</script>

